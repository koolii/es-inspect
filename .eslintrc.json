{
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".mjs"]
      }
    }
  },
  "rules": {
    "semi": [2, "never"],
    "no-console": "off",
    "import/extensions": [
      "error",
      "always",
      {
        "js": "never",
        "mjs": "never"
      }
    ],
    "no-param-reassign": "off"
  }
}